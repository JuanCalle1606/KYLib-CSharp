<Project>
	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFrameworks>netstandard2.1;netstandard2.0</TargetFrameworks>
		<LangVersion>9.0</LangVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>Juan Pablo Calle</Authors>
		<Company>Juan Pablo Calle Grafe</Company>
		<OutputPath>$(SolutionDir)bin\$(Configuration)</OutputPath>
		<Copyright>Copyrigth Â© 2021 Juan Pablo Calle</Copyright>
		<RepositoryUrl>https://github.com/JuanCalle1606/KYLib-CSharp</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<RepositoryBranch>master</RepositoryBranch>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>1.5.6</Version>
		<NeutralLanguage>es</NeutralLanguage>
	</PropertyGroup>

	<PropertyGroup Condition="$(TargetFramework)=='netstandard2.0'">
    	<DefineConstants>$(DefineConstants);NS20</DefineConstants>
 	</PropertyGroup>
 	<PropertyGroup Condition="$(TargetFramework)=='netstandard2.1'">
    	<DefineConstants>$(DefineConstants);NS21</DefineConstants>
 	</PropertyGroup>
</Project>
