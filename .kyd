{
	"Build": {
		"Name": "KYLib.Example",
		"Configuration": "Debug",
		"OutputDir": "{CurrentDir}/bin/{Configuration}/net5.0",
		"Cmd": [
			{
				"Comment": "Build the source code into {OutputDir} using {Task}",
				"RunIn": "{CurrentDir}",
				"Task": "dotnet",
				"Args": "build {Name} -c {Configuration} {OutRedirect} {OutputDir}",
				"OutRedirect": "-o"
			},
			{
				"Comment": "Clear {OutputDir} from autogenerated files",
				"RunIn": "{OutputDir}",
				"Task": "rm",
				"Args": "-r ref *.pdb *.dev.* *.deps.* *.xml",
				"Conditions": [
					{
						"Input": "{Configuration}",
						"NotEqualsTo": "Debug",
						"SystemBased": "Linux"
					}
				]
			}
		]
	}
}