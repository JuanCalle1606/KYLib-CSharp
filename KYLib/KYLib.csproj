<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Juan Pablo Calle</Authors>
    <Company>Juan Pablo Calle Grafe</Company>
    <Title>KYLib</Title>
    <Description>Libreria sencilla de clases y utilidades</Description>
    <Copyright>Copyrigth Â© 2021 Juan Pablo Calle</Copyright>
    <PackageProjectUrl>https://github.com/JuanCalle1606/KYLib-CSharp/tree/main/KYLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JuanCalle1606/KYLib-CSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryBranch>master</RepositoryBranch>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageId>KYLib</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.3.14.5</Version>
    <DocumentationFile>$(ProjectDir)KYLib.xml</DocumentationFile>
    <NeutralLanguage>es</NeutralLanguage>
  </PropertyGroup>
</Project>
